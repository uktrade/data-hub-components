{
  "name": "data-hub-components",
  "version": "0.3.1",
  "license": "MIT",
  "homepage": "https://github.com/uktrade/data-hub-components",
  "description": "Library of React Components used by the Data Hub platform (datahub.trade.gov.uk).",
  "private": false,
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:uktrade/data-hub-components.git"
  },
  "scripts": {
    "start": "start-storybook",
    "build": "rm -rf dist && NODE_ENV=production babel src --out-dir dist --ignore **/*.stories.jsx,**/*.test.jsx,**/*.test.js,**/__snapshots__,**/__tests__",
    "build:storybook": "build-storybook",
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "release": "yarn build && yarn publish"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "@govuk-react/constants": "^0.7.1",
    "axios": "^0.19.0",
    "govuk-react": "^0.7.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react-markdown": "^4.0.8",
    "styled-components": "^4.2.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-info": "^5.0.11",
    "@storybook/addon-knobs": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest": "^24.8.0",
    "mockdate": "^2.0.2",
    "nock": "^10.0.6",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/scripts/setupTests.js"
    ]
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-runtime"
    ]
  }
}
